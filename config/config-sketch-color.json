{
  "source": ["properties/**/*.js"],
  "platforms": {
    "sketchColor": {
      "buildPath": "build/sketch/",
      "files": [{
        "destination": "colorDesignTokens.sketchpalette",
        "format": "sketch/palette/v2",
        "filter": {
          "attributes": {
            "category": "color"
          }
        }
      }],
      "prefix": "ts",
      "transformGroup": "ts/sketch"
    }
  }
}
